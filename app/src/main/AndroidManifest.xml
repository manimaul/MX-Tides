<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mxmariner.tides">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <!--<uses-permission android:name="com.mxmariner.andxtidelib.ACCESS_ANDXTIDE_SERVICE" />-->

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:icon="@drawable/ic_launcher">
        <activity
            android:name="com.mxmariner.activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name="com.mxmariner.andxtidelib.HarmonicsDatabaseService">
            <intent-filter>
                <action android:name="com.mxmariner.andxtidelib.HarmonicsDatabaseService" />
            </intent-filter>
        </service>
    </application>

</manifest>